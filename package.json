{
    "name": "wl-honeycomb-web",
    "author": "Workleap",
    "description": "Shared Honeycomb configuration for web application at Workleap.",
    "private": true,
    "type": "module",
    "license": "Apache-2.0",
    "scripts": {
        "dev": "turbo run dev --filter=./lib",
        "build": "turbo run build --filter=./lib",
        "ci-build": "turbo run build --filter=./lib --cache-dir=node_modules/.cache/turbo",
        "test-no-build": "jest",
        "pretest": "turbo run build --filter=./lib",
        "test": "jest",
        "prelint": "turbo run build",
        "lint": "pnpm run \"/^lint:.*/\"",
        "lint:eslint": "eslint . --max-warnings=-0 --cache --cache-location node_modules/.cache/eslint",
        "lint:knip": "knip --cache --cache-location node_modules/.cache/knip",
        "lint:installed-check": "installed-check --engine-check --version-check",
        "lint:types": "pnpm -r --parallel --include-workspace-root exec tsc",
        "changeset": "changeset",
        "publish-lib": "pnpm changeset",
        "clean": "pnpm -r --parallel --include-workspace-root exec pnpm dlx rimraf dist .turbo .netlify node_modules/.cache",
        "reset": "pnpm clean && pnpm reset:modules",
        "reset:modules": "pnpm -r --parallel --include-workspace-root exec pnpm dlx rimraf node_modules pnpm-lock.yaml",
        "dev-sample": "turbo run dev --filter=./sample",
        "build-sample": "turbo run build --filter=./sample",
        "ci-build-sample": "turbo run build --filter=./sample --cache-dir=node_modules/.cache/turbo",
        "serve-sample": "turbo run serve-build --filter=./sample",
        "deploy-sample": "cross-env NETLIFY=true / --filter=./sample",
        "dev-docs": "retype start",
        "list-outdated-deps": "pnpm outdated -r --format list !eslint !turbo !typescript",
        "update-outdated-deps": "pnpm update -r --latest !eslint !turbo !typescript"
    },
    "devDependencies": {
        "@changesets/changelog-github": "0.5.0",
        "@changesets/cli": "2.27.9",
        "@types/node": "22.9.0",
        "@typescript-eslint/parser": "8.14.0",
        "@workleap/eslint-plugin": "3.2.3",
        "@workleap/typescript-configs": "3.0.2",
        "cross-env": "7.0.3",
        "eslint": "8.57.0",
        "installed-check": "9.3.0",
        "jest": "29.7.0",
        "knip": "5.36.7",
        "retypeapp": "3.6.0",
        "ts-node": "10.9.2",
        "turbo": "1.13.3",
        "typescript": "5.5.4"
    },
    "engines": {
        "node": ">=21.1.0",
        "pnpm": ">=9"
    },
    "packageManager": "pnpm@9.5.0+sha512.140036830124618d624a2187b50d04289d5a087f326c9edfc0ccd733d76c4f52c3a313d4fc148794a2a9d81553016004e6742e8cf850670268a7387fc220c903"
}

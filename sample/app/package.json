{
    "name": "@sample/app",
    "author": "Workleap",
    "version": "0.0.0",
    "description": "Default Honeycomb instrumentation for Workleap's web applications.",
    "private": true,
    "license": "Apache-2.0",
    "type": "module",
    "scripts": {
        "dev": "nodemon",
        "build": "pnpm build:rsbuild && pnpm build:copy-public-files",
        "build:rsbuild": "rsbuild build --config rsbuild.build.ts",
        "build:copy-public-files": "copyfiles -u 1 public/favicon.png dist",
        "serve-build": "pnpm http-server dist -p 8080 -P http://localhost:8080? -c-1"
    },
    "devDependencies": {
        "@rsbuild/core": "1.1.13",
        "@rspack/core": "1.1.8",
        "@tanstack/react-query": "5.59.20",
        "@types/react": "18.3.12",
        "@types/react-dom": "18.3.1",
        "@workleap/browserslist-config": "2.0.1",
        "@workleap/eslint-plugin": "3.2.3",
        "@workleap/rsbuild-configs": "1.0.2",
        "@workleap/typescript-configs": "3.0.2",
        "browserslist": "4.24.2",
        "copyfiles": "2.4.1",
        "eslint": "8.57.0",
        "http-server": "14.1.1",
        "nodemon": "3.1.7",
        "typescript": "5.5.4"
    },
    "dependencies": {
        "@workleap/honeycomb": "workspace:*",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-router-dom": "6.28.0"
    },
    "engines": {
        "node": ">=21.1.0"
    }
}
